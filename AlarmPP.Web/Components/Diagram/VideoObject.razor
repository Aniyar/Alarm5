<svg>
    @if (Object != null)
    {
        <rect x="@X" y="@Y" width="@Object.H" height="@Object.W" fill="blue" stroke-width="1" opacity="0.2" stroke="blue" stroke-dasharray="2,2"></rect>
        <text>
            <text x="@(X+5)" y="@(Y+5)" font-size="10" fill="white">@Object.Name</text>
        </text>
    }
</svg>
